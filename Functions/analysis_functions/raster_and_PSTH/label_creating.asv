function [label_str] = label_creating(name,m)

for i = 1:numel(name)
    if m == 1
        label_str{1,i} = name{i};
    end
    if m == 2
        label_str{1,i} = strcat(name{i},' - before');
        label_str{2,i} = strcat(name{i},' - after');
    end
end
label_str_t = reshape(label_str{:},[],numel(numel(name)*2)
end

